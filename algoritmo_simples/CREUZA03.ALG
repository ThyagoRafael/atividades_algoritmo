Algoritmo "Creuza03"

Var
   celsius, fahrenheit: Real

Inicio
      Escreva("Digite quantos graus, em fahrenheit, esta a temperatura: ")
      Leia(fahrenheit)
      
      celsius <- (fahrenheit - 32) / 1.8
      
      Escreva(fahrenheit, " graus fahrenheit equivale a ", celsius, " graus celsius")

Fimalgoritmo